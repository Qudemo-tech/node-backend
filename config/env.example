# Database Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=24h

# Server Configuration
PORT=5000
NODE_ENV=development

# CORS Configuration
# Note: CORS origins are now configured directly in server.js
# Allowed origins: localhost:3000, localhost:3001, qu-demo.vercel.app, qudemo.com

# Python API Configuration
PYTHON_API_BASE_URL=http://localhost:5001

# Video Processing API Keys
VIMEO_API_KEY=your_vimeo_api_key_here
LOOM_API_KEY=your_loom_api_key_here

# Job Queue Configuration (Pure Node.js)
QUEUE_MAX_CONCURRENT_VIDEOS=2
QUEUE_MAX_CONCURRENT_QA=10
QUEUE_VIDEO_PRIORITY=2
QUEUE_QA_PRIORITY=1
QUEUE_RETRY_ATTEMPTS=3
QUEUE_BACKOFF_DELAY=5000
QUEUE_MAX_MEMORY_MB=1600
QUEUE_JOB_TIMEOUT_MS=300000

# Concurrency Settings
MAX_CONCURRENT_VIDEO_PROCESSING=2
MAX_CONCURRENT_QA_REQUESTS=20
MEMORY_THRESHOLD_MB=1600
REQUEST_TIMEOUT_MS=300000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=200

# File Upload
MAX_FILE_SIZE=100mb
UPLOAD_PATH=./uploads

# Logging
LOG_LEVEL=info
ENABLE_DEBUG_LOGS=true 